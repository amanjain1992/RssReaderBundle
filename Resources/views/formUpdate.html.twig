{% form_theme form 'ClarolineCoreBundle::form_theme.html.twig' %}
<form
    class="form-horizontal"
    id="rss-reader-form"
    action="{{ path('claro_rss_config_update', {'configId': rssConfig.getId()})}}"
    method="post"
    {{ form_enctype(form) }}
>
    <fieldset>
        {{ form_widget(form) }}
    </fieldset>
    <div class="form-actions panel-footer">
        <button type="submit" class="btn btn-primary">
            {{ 'ok'|trans({}, 'platform') }}
        </button>
    </div>
</form>